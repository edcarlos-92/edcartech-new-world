{
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "framework": "nextjs",
  "functions": {
    "pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "VERCEL_ANALYTICS": "0",
    "NEXT_PUBLIC_VERCEL_ANALYTICS": "0"
  }
}
